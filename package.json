{
  "name": "nest-typeorm-template",
  "version": "0.0.1",
  "description": "Nest Starter Template",
  "author": "Iftekhar Salmin Rashid",
  "private": true,
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env STAGE=dev nest start --watch",
    "start:staging": "cross-env STAGE=staging nest start --watch",
    "start:uat": "STAGE=uat nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "STAGE=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "STAGE=test jest --config ./test/jest-e2e.json --watchAll --no-cache",
    "typedoc": "typedoc --options ./typedoc.json",
    "typeorm": "cross-env STAGE=dev ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/configs/ormconfig.ts",
    "migration:create": "npx typeorm migration:create ./src/database/migrations/Migration",
    "migration:generate": "npm run typeorm -- migration:generate ./src/database/migrations/Migration -p",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "prepare": "husky",
    "compile-time": "node measure-compile-time.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.848.0",
    "@aws-sdk/client-secrets-manager": "^3.810.0",
    "@aws-sdk/s3-request-presigner": "^3.635.0",
    "@elastic/elasticsearch": "^8.19.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.20",
    "argon2": "^0.40.3",
    "aws-sdk": "^2.1692.0",
    "bull": "^4.16.5",
    "bullmq": "^5.58.2",
    "cache-manager": "^7.1.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.6",
    "cron": "^4.3.3",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nest-winston": "^1.9.7",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "prom-client": "^15.1.3",
    "pug": "^3.0.3",
    "raw-body": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.4.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.13.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/cookie-parser": "^1.4.7",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.26.5",
    "typedoc-plugin-merge-modules": "^6.0.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
